    <script>
    	function changeAD(isOpen)
    	{
    		var lx_tl_name = "f_tl_";
    		var lx_tl_height = "70";
    		var lx_tl_width = "960";
    		var lx_tl_num =3;
    		var lx_tl_ad = Array(3);
    		lx_tl_ad[0] =lunxun1url;
    		lx_tl_ad[1] ="";
    		lx_tl_ad[2] ="";
    		if(isOpen == true){
    		    var clock=0;
    			for(var i = 0; i < lx_tl_num; i++){
    				if(lx_tl_ad[i]){
    				    clock++;
    				}
    			}
                lx_tl_num = clock;
    			changeAd(lx_tl_name, 'houseDefault_tonglan', lx_tl_ad, lx_tl_num, lx_tl_height, lx_tl_width);
    		}else{
    			for(var i = 0; i < lx_tl_num; i++){
    				document.getElementById(lx_tl_name + (i + 1)).src = lx_tl_ad[i];
    				document.getElementById(lx_tl_name + (i + 1)).height = lx_tl_height;
    				document.getElementById(lx_tl_name + (i + 1)).width = lx_tl_width;
    			}
    		}
    	}
    	$(function(){
			changeAD(true);
		})
    </script>
    	
    
		
