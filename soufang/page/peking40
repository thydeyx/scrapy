var msDefaultShow= "1";
var houseurl = "http://shijiaguangchangjd.fang.com/";
function miaoshaProcess(){
    if(msDefaultShow==1){
        $('#miaoshaBox').toggle();
	}else{
	    window.open(houseurl+"#daohang");
	}
}
function yhqProcess(){
    if(msDefaultShow==1){
        $('#youhuiquanBox').toggle();
	}else{
	    window.open(houseurl+"#daohang");
	}
}
</script>

