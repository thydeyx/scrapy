<script>
var currNewcode = '1010786043';
$(function(){
    $.ajax({
        type:'get',
        dataType:'jsonp',
        <!-- url:"http://shijiaguangchangjd.fang.com/house/newhousechat/zygwTel400.php", -->
        url:newhousedomain+"/house/newhousechat/zygwTel400.php",
        data:{newcode:currNewcode},
        complete:function(){},
        error:function(){},
        success:function(msgRes){
            // 动态修改页面内容
			var trHtml = '';
            var rows = msgRes.length;
            for(var i=0; i<rows; i++) {
                trHtml += "<tr><td><strong>" +msgRes[i]['tel400']+"</strong> 找 <a title='"+msgRes[i]['realname']+"' href='http://www.fang.com/xinfangbang/u/"+msgRes[i]['user_id']+".htm' target='_blank'>"+msgRes[i]['realname'].substring(0, 3)+"</a></td></tr>";
            }	
            
			$("#chat_400_table").append(trHtml);
			if(rows>0){
			    $("#chatimg").attr('src','http://img1.soufun.com/house/detail_php/tel400talk/dh_2.gif');
			}else{
			    $("#chatimg").attr('src','http://img1.soufun.com/house/detail_php/tel400talk/dh_12.gif');
			}
        }
    });
});
</script>
	
