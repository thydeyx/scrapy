	function Show_Cookie(){
        var tmp = new Array();
        Xml.Request("/house/shop/ajax_shop.php?time=" + new Date().getTime(), null, call_back_cookie,tmp);
	}
	function call_back_cookie()
	{
		var coo_data = this.responseText;//获取cookie
		if(coo_data)
		{
			var obj = eval('(' + coo_data + ')');
			var used_=0;
			if(obj.data.length>0)
			{
				var view_template="";
				var show_num = obj.data.length > 9 ? 9 : obj.data.length;
				for(i=0;i<show_num;i++)
				{
					var cookie_proj_code = obj.data[i].keyname;
					var cookie_proj_info = obj.data[i].value.split("ζ");
					var cookie_proj_name = cookie_proj_info[0];
					var cookie_proj_price = cookie_proj_info[1];
					if(cookie_proj_code != "" && cookie_proj_name != "")
					{
						view_template = view_template + "<div class='bybbkmiddlenr01'><input type='checkbox' value='" + cookie_proj_code + "' class='bybbkmiddleinput' /><strong><a href='http://"+ shopdomain + "/house/" + ucity + "_" + cookie_proj_code + ".htm' target='_blank'>" + cookie_proj_name + "</a></strong></div>";
					}
				}
				document.getElementById('browse_mark4byb').innerHTML = view_template;
				//$("#browse_mark4byb").html(view_template);
				$("#browse_mark4byb").after("<div class='sydcico seebtns'><a href='javascript:void(0);' onclick='submit_byb();'></a></div>");
			}
		}
	}
	Show_Cookie();
	function submit_byb(){
		var url = '/house/shop/houseContrast.php?str_id=';
		var inputs = document.getElementsByTagName("input");
		var num = 0;
		for(var i = 0; i < inputs.length; i++){
			if(inputs[i].type == "checkbox"){
				if(inputs[i].checked == true){
					url += inputs[i].value + '|';
					num++;
				}
			}
		}
		if(num < 2 || num > 4){
			alert('请选择2-4个楼盘进行比较！');
		}
		else {
		    window.open(url);    
		}
	}
	function SetProjSubscriptionDisplay(state){
		var tempDiv = document.getElementById("ProjSubscriptionDiv");
		if(tempDiv){
