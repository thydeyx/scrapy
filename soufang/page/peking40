				tempDiv.style.display = 'block';
			}else{
				tempDiv.style.display = 'none';
			}
		}
	}
	function moreBoxto_n(num){
		var tid = "moreBox" + num;
		document.getElementById(tid).style.display = 'block';
		if(document.getElementById('mobile').value != '' || document.getElementById('mobile').value != '请输入手机号码'){
			document.getElementById('mobile').value = '请输入手机号码';
		}
		if(document.getElementById('yanzheng2').value != ''){
			document.getElementById('yanzheng2').value = '';
		}
	}
	function sendmsg(){
		var code = document.getElementById('yanzheng2').value;
		var mobile = document.getElementById('mobile').value;
		exp = /^[0-9]{11}$/;
		if(mobile != ''){
			if(mobile.match(exp) == null){
				alert('请输入正确的手机号码')
				return false;
			}
		}else{
			alert('请输入手机号码')
			return false;	
		}if(code == ''){
			alert('请输入验证码');
			return false;	
		}else{
			if(code.match(/^[0-9]{6}$/) == null){
				alert('请正确输入验证码')
				return false;
			}
		}
		var content = encodeURIComponent(document.getElementById('mobilemsg').innerHTML);
	    Xml.Request("/house/shop/houseDetail_send.php?code=" + code + "&mobile=" + mobile + "&newcode=1010786043&msg=" + content + "&projname=" + encodeURIComponent('金地世家广场') + "&time=" + new Date().getTime(), null, getRs, null);
	}
	function getcode(){
		if(document.getElementById("sendinfo").style.display == "block"){
	　　	document.getElementById("sendinfo").style.display = "none";
		}else{
			document.getElementById("sendinfo").style.display = "block";
		}
		document.getElementById('sendinfo').style.height = '240px';
        document.getElementById('iframe_sendinfo').style.height = '240px';
		document.getElementById("iframe_sendinfo").src = "/house/shop/mobilecode.htm?t=" + new Date().getTime();
	}
	function getRs(){
		if(this.responseText == '楼盘点播成功'){
			alert(this.responseText);
			document.getElementById('moreBox37').style.display = "none";
			document.getElementById('mobile').value = '请输入手机号';
			document.getElementById('yanzheng2').value = '';
		}
		else if(this.responseText != "")
			alert(this.responseText);
		else
			alert("楼盘点播失败")
	}
	function _post400action(newcode, type1, type2, value){
		var type;
		if(type1 == true){
			type = "telephone";
			exp = /^\s*[-\+]?\d+\s*$/;
			if(value != ''){
				if(value.match(exp) == null){
					alert('请输入正确的手机号码')
					return false;
				}
			}
		}else if(type2 == true){
			type = "email";
			exp = /^[_a-z0-9]+@([_a-z0-9]+\.)+[a-z0-9]{2,3}$/;
			if(value.match(exp) == null){
				alert('请输入正确的邮箱')
				return false;
			}
		}
		if(type != "" && value != ""){
			Xml.Request("/house/shop/post_400.php?newcode=" + newcode + "&type=" + type + "&content=" + value, null, alert('提交成功！'), null);
		}
	}
