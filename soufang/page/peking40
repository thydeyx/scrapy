<script language="javascript">
function getScrollingPosition( ) {
    var position = [0,0];
    if ( typeof window.pageYOffset != 'undefined') {
       position = [ window.pageXOffset, window.pageYOffset];
    } else if ( typeof document.documentElement.scrollTop != 'undefined' && document.documentElement.scrollTop > 0) {
          position = [ document.documentElement.scrollLeft,document.documentElement.scrollTop];                       
    } else if (typeof document.body.scrollTop != 'undefined') {
          position = [ document.body.scrollLeft , document.body.scrollTop];
    }
    return position;
}
var citySuo = "bj";
var cityurlencode = "%B1%B1%BE%A9";
var beginUbRequest = false;
function initUbRequest()
{
    _ub.request('vwn.key,vwn.position,vwn.subway,vwn.genre,vwn.unitprice,vwn.feature,vwn.salestatus,vwn.housetype,vwn.loopline,vwn.opentime', function(){
			_ub.load(3);//取时间最近的数据
			var ubKeyStr = _ub['vwn.key'].v+';'+_ub['vwn.key'].d+';'+_ub['vwn.key'].m;
			var ubPositionStr = _ub['vwn.position'].v+';'+_ub['vwn.position'].d+';'+_ub['vwn.position'].m;
			var ubSubwayStr = _ub['vwn.subway'].v+';'+_ub['vwn.subway'].d+';'+_ub['vwn.subway'].m;
			var ubGenreStr = _ub['vwn.genre'].v+';'+_ub['vwn.genre'].d+';'+_ub['vwn.genre'].m;
			var ubUnitpriceStr = _ub['vwn.unitprice'].v+';'+_ub['vwn.unitprice'].d+';'+_ub['vwn.unitprice'].m;
			var ubFeatureStr = _ub['vwn.feature'].v+';'+_ub['vwn.feature'].d+';'+_ub['vwn.feature'].m;		
			var ubSalestatusStr = _ub['vwn.salestatus'].v+';'+_ub['vwn.salestatus'].d+';'+_ub['vwn.salestatus'].m;
			var ubHousetypeStr = _ub['vwn.housetype'].v+';'+_ub['vwn.housetype'].d+';'+_ub['vwn.housetype'].m;
			var ubLooplineStr = _ub['vwn.loopline'].v+';'+_ub['vwn.loopline'].d+';'+_ub['vwn.loopline'].m;
			var ubOpentimeStr = _ub['vwn.opentime'].v+';'+_ub['vwn.opentime'].d+';'+_ub['vwn.opentime'].m;
			var ubAllCondition = ubKeyStr+'|'+ubPositionStr+'|'+ubSubwayStr+'|'+ubGenreStr+'|'+ubUnitpriceStr+'|'+ubFeatureStr+'|'+ubSalestatusStr+'|'+ubHousetypeStr+'|'+ubLooplineStr+'|'+ubOpentimeStr;
        
			var requestUrl = "/house/web/ubRequest.php?num=5&frompage=xfxq&from=xfxq&condition="+ubAllCondition+"&newcode="+newcode+"&time=" + new Date().getTime();
			Xml.Request(requestUrl, null, callback_displayMaylike, null);
			/*var openUrl1 = requestUrl+"&DEBUG=1";
			window.open(openUrl1,'_black')*/
		});
    _ub.request('n');
}
function getN9District(num)
{
    var returnString = ""; var getNum = 0;
    for(var i=0; i<20; i++)
    {
        var string = _ub.n9;
        _ub.load(0);
        var stringArr = string.split("^");
        if(stringArr[2] == '1')
        {
            returnString += stringArr[0]+"|";
            getNum++;
            if(getNum == num)
            {
                return returnString;
            }
        }
    }
    return returnString;
}
function padLeft(str,len)
{
    str = str.toString();
    if(str.length >= len)
        return str;
    else
        return padLeft("0" +str,len);
} 
function callback_displayMaylike()
{
    var getData = this.responseText;
    if(getData)
    {
        var obj = eval('(' + getData + ')');
        var htmlContent = "";
        for(var i=0; i<obj.length; i++)
        {
			var liContent = '';
			liContent += '<ul class="Y_like" style="padding-bottom:10px;">';
			liContent += '<li class="imgbor" style="position: relative;height: 117px;">';
			liContent += '<a href="'+obj[i]['imgLink']+'" target="_blank">';
			liContent += '<img class="imgts" src="'+obj[i]['picture_thumb']+'" alt="'+obj[i]['district']+' '+obj[i]['title']+'" width="177px" height="117px;">';
			liContent += '</a>';
			liContent += '<span style="left: 50%;top: 44px;position: absolute;height: 24px;text-align: center;line-height: 24px;color: #fff;font-weight: bold;transform: translate(-50%,0);margin: 0 auto;opacity: 1;background-color: rgba(0,0,0,0.6);padding: 0 10px;border-radius: 4px;">'+obj[i]['projtype']+'</span>';
			if(obj[i]['pcadnewSign']){
				liContent += '<img src="http://imgd2.soufunimg.com/2015/12/25/25k/71ea846c543c4065b86e95b5b3244712.png" style="position:absolute; top:0px; right:-2px; ">';
			}
			liContent += '</li>';
			
			liContent += '<li style="font-weight: bold;color: #333;width:160px;height: 24px;line-height: 24px;-ms-text-overflow: ellipsis;text-overflow: ellipsis;overflow: hidden;white-space:nowrap;">';
			liContent += '<a href="'+obj[i]['houseurl']+'" target="_blank">'+obj[i]['district']+'-'+obj[i]['title']+'</a>';
			liContent += '</li>';
			
			liContent += '<div style="clear:both"></div>';
			liContent += '<li style="position: relative;">';
			
			if(obj[i]['price_unit'] == '元/平方米' || !obj[i]['price_unit']){
					var price_unit = '元/O';
			}else{
				var price_unit = obj[i]['price_unit'];
			}
			liContent +='<span style="font-weight: bold;">均价：</span>';
			if(obj[i]['price_num']){				
				liContent +='<i style="font-style: normal;color: #c00;">'+obj[i]['price_num']+price_unit+'</i>';				
			}else{
				liContent +='<i style="font-style: normal;color: #c00;">价格待定</i>';				
			}
			if(obj[i]['squares']){
				liContent +='<em style="color: #999;float: right;position:absolute;right:0;">'+obj[i]['squares']+'O</em>';
			}
			liContent +='</li>';
			if(obj[i]['louPanYHInfoTag']){
				liContent += '<li><em style="float: left;height: 22px;line-height: 22px;padding:0 10px;border:1px solid #dbdbdb;">'+obj[i]['louPanYHInfoTag']+'</em></li>';
			}
			liContent += '</ul>';
			htmlContent += liContent;
		}
        document.getElementById('divMaylikeUl').innerHTML = htmlContent;
    }
    document.getElementById('divMaylike').style.display = '';
}
var maylikeTop = document.getElementById('divMaylikePosition').offsetTop;
window.onscroll = function () {
    var scrollPos = getScrollingPosition();
    if(scrollPos[1] + window.screen.height > maylikeTop && beginUbRequest == false)
    {
        beginUbRequest = true;
        initUbRequest();
    }
};
</script>

