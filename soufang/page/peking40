					//alert(msgRes.agree_num);
						var zanHtml = '';
						zanHtml = '<span id="piao_'+tid+'" class="t04 tf">+1</span><a class="t03 mr05" id="agree_'+tid+'" href="javascript:void(0);" title="取消赞" onclick="delAgree('+tid+');"><img class="best" src="http://img1.soufun.com/house/detail_php/images/alf.gif" width="12" height="12" /></a>('+msgRes.agree_num+')';
						$("#zanbox_"+tid).html(zanHtml);
						$("#piao_"+tid).hide(2000);
					}
					else  {
						alert(msgRes.msg);
					}
				}
			});	
	}
}
</script>


<script language="javascript">
	function changeAD(isOpen){
		var lx_tl_name = "f_tl_";
		var lx_tl_height = "70";
		var lx_tl_width = "960";
		var lx_tl_num = 2;
		var lx_tl_ad = Array(1);
		lx_tl_ad[0] = "";
		lx_tl_ad[1] = "";
		if(isOpen == true){
			for(var i = 0; i < lx_tl_num; i++){
				if(lx_tl_ad[i] == ""){
					lx_tl_num = i;
					break;
				}
			}
			changeAd(lx_tl_name, 'shop_detail_tonglan', lx_tl_ad, lx_tl_num, lx_tl_height, lx_tl_width);
		}else{
			for(var i = 0; i < lx_tl_num; i++){
				if(lx_tl_ad[i] != ""){
					document.getElementById(lx_tl_name + (i + 1)).src = lx_tl_ad[i];
					document.getElementById(lx_tl_name + (i + 1)).height = lx_tl_height;
					document.getElementById(lx_tl_name + (i + 1)).width = lx_tl_width;
				}
			}
		}
	}
changeAD(true);
</script>

</body>
</html>
